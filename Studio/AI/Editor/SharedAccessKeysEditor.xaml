<sk:TabDocument x:Class="Skill.Studio.AI.Editor.SharedAccessKeysEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"              
             xmlns:sk="clr-namespace:Skill.Studio"
             xmlns:aieditor="clr-namespace:Skill.Studio.AI.Editor"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="461" d:DesignWidth="808" x:Name="This" >
    
    <sk:TabDocument.CommandBindings>
        <CommandBinding Command="ApplicationCommands.Undo"
                  Executed="UndoCmdExecuted"
                  CanExecute="UndoCmdCanExecute"/>
        <CommandBinding Command="ApplicationCommands.Redo"
                  Executed="RedoCmdExecuted"
                  CanExecute="RedoCmdCanExecute"/>
    </sk:TabDocument.CommandBindings>
    <Grid Background="{StaticResource ControlBg}"  >
        <aieditor:SharedAccessKeysEditorControl Margin="10" MaxWidth="800" x:Name="_KeysControl" ViewModel="{Binding ElementName=This, Path=SharedAccessKeys}" />        
    </Grid>
</sk:TabDocument>
