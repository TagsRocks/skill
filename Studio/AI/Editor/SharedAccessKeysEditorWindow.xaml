<Window x:Class="Skill.Studio.AI.Editor.SharedAccessKeysEditorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:extToolkit="clr-namespace:Xceed.Wpf.Toolkit.PropertyGrid;assembly=WPFToolkit.Extended"
        xmlns:dmai="clr-namespace:Skill.DataModels.AI;assembly=DataModels"
        xmlns:aieditor="clr-namespace:Skill.Studio.AI.Editor"
        Title="Shared AccessKeys Editor" Height="406" Width="722" x:Name="This" WindowStartupLocation="CenterScreen" Background="{StaticResource ControlBg}" >
    <Window.InputBindings>
        <KeyBinding Command="ApplicationCommands.Undo" Gesture="CTRL+Z" />
        <KeyBinding Command="ApplicationCommands.Redo" Gesture="CTRL+Y" />
    </Window.InputBindings>
    <Window.CommandBindings>
        <CommandBinding Command="ApplicationCommands.Undo"
                  Executed="UndoCmdExecuted"
                  CanExecute="UndoCmdCanExecute"/>
        <CommandBinding Command="ApplicationCommands.Redo"
                  Executed="RedoCmdExecuted"
                  CanExecute="RedoCmdCanExecute"/>
    </Window.CommandBindings>

    <Grid>
        <aieditor:SharedAccessKeysEditorControl x:Name="_KeysControl" ViewModel="{Binding ElementName=This, Path=SharedAccessKeys}" Margin="10" />
    </Grid>
</Window>
